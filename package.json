  "devDependencies": {
    "@types/jest": "21.1.2",
    "@types/minimist": "1.2.0",
    "@types/node": "8.0.32",
    "@types/react": "16.0.9",
    "@types/react-addons-test-utils": "0.14.20",
    "@types/react-dom": "16.0.0",
